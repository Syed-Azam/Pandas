   "source": [
    "import pandas as pd\n",
    "import glob\n",
    "a = glob.glob(\"csvs/*.csv\")\n",
    "l = []\n",
    "for f in a:\n",
    "    df = pd.read_csv(f, index_col=None, header=0)\n",
    "    l.append(df)\n",
    "\n",
    "f = pd.concat(l, axis=0, ignore_index=True)\n",
    "display(f)\n",
    "f.to_csv(\"csvs/final.csv\", index=False)"
   ]
